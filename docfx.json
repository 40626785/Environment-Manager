{
    "metadata": [
    {
        "src": [
        {
            "files": ["EnvironmentManager/EnvironmentManager.csproj"],
            "exclude": ["bin/**", "obj/**"]
        }
        ],
        "dest": "api",
        "genereateToc": true,
        "filter": "docfxConfigFilter.yml",
        "properties": {
            "TargetFramework": "net8.0"
        }
    }
    ],
    "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md",
          "articles/**.md",
          "api/**.yml",
          "api/index.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Environment Manager Documentation",
      "_appFooter": "Generated by DocFX",
      "_enableSearch": true,
      "_disableContribution": true,
      "_disableApiPage": true,
      "_basePath": "/Environment-Manager"
    },
    "maxParallelism": 4,
    "sitemap": {
      "baseUrl": "https://40626785.github.io/Environment-Manager"
    }
  }
}
