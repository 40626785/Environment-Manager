{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md",
          "articles/sensors.md"
        ],
        "exclude": [
          "articles/intro.md",
          "**/intro.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Environment Manager",
      "_enableSearch": true,
      "_navbarItems": [
        {
          "name": "Home",
          "href": "index.md"
        },
        {
          "name": "Sensor Management",
          "href": "articles/sensors.md"
        },
        {
          "name": "GitHub",
          "href": "https://github.com/40626785/Environment-Manager"
        }
      ]
    },
    "template": [
      "default"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
