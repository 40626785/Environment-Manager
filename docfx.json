{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md",
          "articles/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Environment Manager",
      "_enableSearch": true
    },
    "template": [
      "default"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
