{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md",
          "articles/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico",
          ".nojekyll"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/custom"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Environment Manager Documentation",
      "_enableSearch": true,
      "_appFaviconPath": "favicon.ico",
      "_appLogoPath": "logo.svg",
      "_appFooter": "Generated by DocFX",
      "_gitContribute": {
        "repo": "https://github.com/40626785/Environment-Manager",
        "branch": "main"
      }
    }
  }
}
